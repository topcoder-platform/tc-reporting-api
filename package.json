{
  "name": "tc-reproting-api",
  "version": "1.0.0",
  "description": "lookerApi wrapper service",
  "main": "index.js",
  "engines": {
    "node": ">=6.9"
  },
  "scripts": {
    "dev": "PORT=3000 nodemon -w src --exec \"babel-node src --presets es2015\" | ./node_modules/.bin/bunyan",
    "build": "babel src -d dist --presets es2015",
    "start": "node dist",
    "prestart": "npm run -s build",
    "lint": "./node_modules/.bin/eslint src"
  },
  "eslintConfig": {
    "plugins": [],
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "mocha": true
    },
    "globals": {
      "Promise": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.0",
    "config": "^1.20.1",
    "continuation-local-storage": "^3.1.7",
    "cors": "^2.8.5",
    "express": "^4.13.4",
    "express-list-routes": "^0.1.4",
    "express-request-id": "^1.1.0",
    "express-validation": "^0.6.0",
    "jackrabbit": "^4.3.0",
    "joi": "^8.0.5",
    "lodash": "^4.17.21",
    "tc-core-library-js": "^1.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.11.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "bunyan": "^1.8.1",
    "eslint": "^3.2.2",
    "eslint-plugin-import": "^1.12.0"
  }
}
